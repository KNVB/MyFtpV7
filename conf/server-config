#FTP Server Configurations
#FTP Server Console Heading
ftpServerConsoleHeading=MyFtpServer admin. console
#It specify the concrete subclass of com.myftpserver.abstracts.FileManager.
fileManager.classname=com.myftpserver.impl.MyFileManager

#It specify the concrete subclass of com.myftpserver.abstracts.UserManager.
userManager.classname=com.myftpserver.impl.MyUserManager

#It specify the concrete subclass of com.myftpserver.abstracts.FtpServerConfig.
ftpServerConfig.classname=com.myftpserver.impl.MyFtpServerConfig

#It specify the port no. that server listen
port=21

#It specify maximum no. of concurrent connection of server that accept
maxConnection=10

#It specify whether the server support Passive Mode or not
supportPassiveMode=true

#It specify a range port that support Passive Mode operation
#passivePortRange=1234,4567-4576,4466
passivePortRange=1232-1234

#It specify client command channel connection time out in second
commandChannelConnectionTimeOut=900
#commandChannelConnectionTimeOut=10

#It specify the server default locale
serverLocale=en_us
#serverLocale=zh_tw

#It specify the IP address which server binded
#bindAddress=127.0.0.1